{
  "name": "random-distrib",
  "version": "1.0.0",
  "description": "Random distribution generator box",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server -c ./configs/lite-server-config.json\" ",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor ./configs/protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server -c ./configs/lite-server-config.json",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start ./configs/karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "keywords": ["random", "distribution"],
  "author": "Andreas Abdi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndreasAbdi/randomDistrib"
  },
  "bugs": {
    "url": "https://github.com/AndreasAbdi/randomDistrib/issues"
  },
  "dependencies": {
    "@angular/common": "~2.2.1",
    "@angular/compiler": "~2.2.1",
    "@angular/core": "~2.2.1",
    "@angular/forms": "~2.2.1",
    "@angular/http": "~2.2.1",
    "@angular/platform-browser": "~2.2.1",
    "@angular/platform-browser-dynamic": "~2.2.1",
    "@angular/router": "~3.2.1",
    "angular-in-memory-web-api": "~0.1.15",
    "ng2-charts": "~1.4.1",
    "chart.js": "~2.4.0",
    "systemjs": "0.19.40",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.10",
    "typings": "^2.0.0",

    "angular-cli": "1.0.0-beta.21",
    "@angular/compiler-cli": "2.2.1",
    "codelyzer": "~1.0.0-beta.3",

    "canonical-path": "0.0.2",
    "http-server": "^0.9.0",
    "tslint": "^3.15.1",
    "lodash": "^4.16.4",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "4.0.9",
    "webdriver-manager": "10.2.5",
    "rimraf": "^2.5.4",

    "@types/angular": "^1.5.16",
    "@types/jquery": "^2.0.33",
    "@types/node": "^6.0.46",
    "@types/jasmine": "^2.5.36",
    "@types/selenium-webdriver": "^2.53.33"
  }
}
